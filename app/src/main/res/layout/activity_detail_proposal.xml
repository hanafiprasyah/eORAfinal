<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DetailProposal"
    android:background="@color/ghostwhite">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarDetailProposal"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:id="@+id/btnBack_detailProposal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:background="@drawable/bg_btnripple"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:text="DETAIL PROPOSAL"
                android:textSize="16sp"
                android:textColor="@color/black"
                android:drawableStart="@drawable/ic_back_ios"
                android:paddingBottom="5dp"
                android:paddingTop="5dp"
                android:paddingStart="10dp"
                android:paddingEnd="5dp"
                android:clickable="true"
                android:focusable="true" />
            <TextView
                android:id="@+id/tv_IdProposalDetailProposal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:visibility="gone"/>
        </RelativeLayout>
    </androidx.appcompat.widget.Toolbar>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedView_detailProposal"
        android:layout_below="@+id/toolbarDetailProposal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <!--DataPribadi-->
            <LinearLayout
                android:id="@+id/dataPribadi_detailProposal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:paddingRight="15dp">
                <!--NIM dan NAMA-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cv1_detailProposal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:cardCornerRadius="20dp"
                    app:cardBackgroundColor="@color/ghostwhite"
                    android:layout_margin="8dp">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="20dp">
                        <ImageView
                            android:id="@+id/fotoMahasiswa_detailProposal"
                            android:layout_width="250dp"
                            android:layout_height="250dp"
                            android:scaleType="centerInside"
                            android:layout_marginStart="10dp"
                            android:layout_marginEnd="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_centerHorizontal="true"/>
                        <View
                            android:id="@+id/separator1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_below="@+id/fotoMahasiswa_detailProposal"
                            android:background="@color/cardview_shadow_start_color"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="20dp"
                            tools:ignore="PrivateResource" />
                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_below="@+id/separator1">
                            <LinearLayout
                                android:id="@+id/LayoutNIMdanNama_detailProposal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:weightSum="8"
                                android:orientation="horizontal"
                                android:layout_centerHorizontal="true">
                                <TextView
                                    android:id="@+id/namaMahasiswa_detailProposal"
                                    android:layout_width="0dp"
                                    android:layout_weight="4"
                                    android:layout_height="wrap_content"
                                    android:textStyle="bold"
                                    android:textColor="@color/black"
                                    android:textSize="14sp"
                                    android:lines="2"
                                    android:layout_marginStart="20dp"
                                    android:layout_gravity="end"
                                    android:gravity="center"
                                    android:text="Memuat .."/>
                                <TextView
                                    android:id="@+id/NIMMahasiswa_detailProposal"
                                    android:layout_width="0dp"
                                    android:layout_weight="4"
                                    android:layout_height="wrap_content"
                                    android:textStyle="italic|bold"
                                    android:textSize="14sp"
                                    android:lines="2"
                                    android:textColor="@color/black"
                                    android:layout_gravity="start"
                                    android:layout_marginEnd="20dp"
                                    android:gravity="center"
                                    android:text="Memuat .."/>
                            </LinearLayout>
                        </RelativeLayout>
                    </RelativeLayout>
                </androidx.cardview.widget.CardView>

                <!--NamaAyah-->
                <LinearLayout
                    android:id="@+id/layoutNamaAyah"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etNamaAyah_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:inputType="textCapWords"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="Nama Ayah"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--NamaIbu-->
                <LinearLayout
                    android:id="@+id/layoutNamaIbu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etNamaIbu_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:inputType="textCapWords"
                        android:layout_height="wrap_content"
                        android:hint="Nama Ibu"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--PekerjaanAyah-->
                <LinearLayout
                    android:id="@+id/layoutPekerjaanAyah"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etPekerjaanAyah_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:inputType="textCapWords"
                        android:layout_height="wrap_content"
                        android:hint="Pekerjaan Ayah"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--PekerjaanIbu-->
                <LinearLayout
                    android:id="@+id/layoutPekerjaanIbu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etPekerjaanIbu_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:inputType="textCapWords"
                        android:layout_height="wrap_content"
                        android:hint="Pekerjaan Ibu"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--JumlahSaudara-->
                <LinearLayout
                    android:id="@+id/layoutJumsau"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etJumlahSaudara_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:hint="Jumlah Saudara"
                        android:inputType="number"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--AnakKe-->
                <LinearLayout
                    android:id="@+id/layoutAnakke"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etAnakKe_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:hint="Anak ke-"
                        android:inputType="number"
                        app:met_floatingLabel="highlight"/>
                    <ImageView
                        android:id="@+id/info_anakke"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ic_informationbublered12"
                        android:layout_marginTop="10dp"
                        android:layout_marginStart="20dp"/>
                </LinearLayout>
                <!--PenghasilanOrtu-->
                <LinearLayout
                    android:id="@+id/layoutPenghasilanOrtu"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etPenghasilan_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="Penghasilan Orang Tua per Bulan"
                        android:inputType="number"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--UKT-->
                <LinearLayout
                    android:id="@+id/layoutUKT"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etUkt_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="Biaya UKT per Semester"
                        android:inputType="number"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--NamaBank-->
                <LinearLayout
                    android:id="@+id/layoutNamaBank"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etNamaBank_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="Nama Bank : Nagari/BRI/.."
                        android:inputType="textCapWords"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--NoRek-->
                <LinearLayout
                    android:id="@+id/layoutNorek"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etNorek_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="Nomor Rekening"
                        android:maxLength="18"
                        android:inputType="number"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--AlamatDomisili-->
                <LinearLayout
                    android:id="@+id/layoutAlamatDomisili"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etAlamatDomisili_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="Alamat Domisili"
                        android:textSize="16sp"
                        android:inputType="textLongMessage"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--alamatKTP-->
                <LinearLayout
                    android:id="@+id/layoutAlamatKTP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginBottom="30dp"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etAlamatKTP_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="Alamat sesuai KTP"
                        android:textSize="16sp"
                        android:inputType="textLongMessage"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>

                <!--IPK-->
                <LinearLayout
                    android:id="@+id/layoutIPK"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etIPK_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="IPK Mahasiswa"
                        android:textSize="16sp"
                        android:inputType="number"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>
                <!--Semester-->
                <LinearLayout
                    android:id="@+id/layoutSemester"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_marginBottom="30dp"
                    android:weightSum="6">
                    <com.airbnb.lottie.LottieAnimationView
                        android:layout_weight="1"
                        android:layout_width="0dp"
                        android:padding="10dp"
                        android:layout_height="match_parent"
                        app:lottie_autoPlay="true"
                        app:lottie_loop="true"
                        app:lottie_fileName="arrowright.json"/>
                    <com.rengwuxian.materialedittext.MaterialEditText
                        android:id="@+id/etSemester_detailProposal"
                        android:layout_weight="5"
                        android:layout_width="0dp"
                        android:layout_marginEnd="15dp"
                        android:layout_height="wrap_content"
                        android:hint="Semester"
                        android:textSize="16sp"
                        android:inputType="number"
                        app:met_floatingLabel="highlight"/>
                </LinearLayout>

                <!--FileBerkas-->
                <LinearLayout
                    android:id="@+id/detailProposal_fileBerkas"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:paddingLeft="15dp"
                    android:paddingRight="15dp"
                    android:layout_marginBottom="10dp">
                    <androidx.cardview.widget.CardView
                        android:id="@+id/cv_fileDetailProposal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:cardCornerRadius="20dp"
                        app:cardBackgroundColor="@color/ghostwhite"
                        android:layout_margin="8dp">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">
                            <TextView
                                android:id="@+id/tv_titleFileBerkasDetailProposal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:layout_marginBottom="10dp"
                                android:gravity="center"
                                android:text="KUMPULAN BERKAS"
                                android:autoSizeMaxTextSize="16sp"
                                android:autoSizeMinTextSize="14sp"
                                android:textSize="16sp"/>
                            <View
                                android:id="@+id/view_separatorFileBerkasDetailProposal"
                                android:layout_width="10dp"
                                android:layout_height="10dp"
                                android:layout_below="@+id/tv_titleFileBerkasDetailProposal"
                                android:background="@drawable/circledot"
                                android:layout_centerHorizontal="true"
                                android:layout_marginBottom="20dp"/>
                            <LinearLayout
                                android:id="@+id/LL_fileBerkas"
                                android:layout_below="@+id/view_separatorFileBerkasDetailProposal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">
                                <!--LHS-->
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    android:layout_gravity="center">
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Lembar Hasil Studi :"
                                        android:textSize="14sp"
                                        android:layout_marginEnd="10dp"
                                        android:autoSizeMaxTextSize="14sp"
                                        android:autoSizeMinTextSize="12sp"/>
                                    <TextView
                                        android:id="@+id/url_FileLHSDetailProposal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:visibility="gone"/>
                                    <ImageView
                                        android:id="@+id/iv_fileLembarHasilStudiDetailProposal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="10dp"
                                        android:src="@drawable/ic_file"
                                        android:clickable="true"
                                        android:focusable="true"/>
                                </LinearLayout>

                                <!--KRS-->
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:layout_gravity="center">
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Kartu Rencana Studi :"
                                        android:textSize="14sp"
                                        android:layout_marginEnd="10dp"
                                        android:autoSizeMaxTextSize="14sp"
                                        android:autoSizeMinTextSize="12sp"/>
                                    <TextView
                                        android:id="@+id/url_FileKRSDetailProposal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:visibility="gone"/>
                                    <ImageView
                                        android:id="@+id/iv_fileKRSDetailProposal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="10dp"
                                        android:src="@drawable/ic_file"
                                        android:clickable="true"
                                        android:focusable="true"/>
                                </LinearLayout>

                                <!--UKT-->
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:layout_gravity="center">
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Pembayaran UKT :"
                                        android:textSize="14sp"
                                        android:layout_marginEnd="10dp"
                                        android:autoSizeMaxTextSize="14sp"
                                        android:autoSizeMinTextSize="12sp"/>
                                    <TextView
                                        android:id="@+id/url_FileUKTDetailProposal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:visibility="gone"/>
                                    <ImageView
                                        android:id="@+id/iv_fileUKTDetailProposal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="10dp"
                                        android:src="@drawable/ic_file"
                                        android:clickable="true"
                                        android:focusable="true"/>
                                </LinearLayout>

                                <!--Prestasi-->
                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:gravity="center"
                                    android:layout_marginTop="20dp"
                                    android:layout_marginBottom="25dp"
                                    android:layout_gravity="center">
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Prestasi Mahasiswa :"
                                        android:textSize="14sp"
                                        android:layout_marginEnd="10dp"
                                        android:autoSizeMaxTextSize="14sp"
                                        android:autoSizeMinTextSize="12sp"/>
                                    <TextView
                                        android:id="@+id/url_FilePrestasiDetailProposal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:visibility="gone"/>
                                    <ImageView
                                        android:id="@+id/iv_filePrestasiDetailProposal"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="10dp"
                                        android:src="@drawable/ic_file"
                                        android:clickable="true"
                                        android:focusable="true"/>
                                </LinearLayout>
                            </LinearLayout>
                        </RelativeLayout>
                    </androidx.cardview.widget.CardView>
                </LinearLayout>
            </LinearLayout>

            <!--AKSI STATUS PROPOSAL-->
            <LinearLayout
                android:id="@+id/layoutPilihan_detailProposal"
                android:layout_below="@+id/dataPribadi_detailProposal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center"
                android:paddingLeft="15dp"
                android:paddingRight="15dp">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="Kelola Status Proposal :"
                    android:layout_marginBottom="20dp"/>
                <Button
                    android:id="@+id/btnTerimaProposal_detailProposal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="T E R I M A"
                    android:textColor="@color/ghostwhite"
                    android:background="@drawable/et_bgmhs"
                    android:padding="10dp"
                    android:layout_marginBottom="20dp"
                    android:enabled="false"/>
                <Button
                    android:id="@+id/btnPerbaikanProposal_detailProposal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="P E R B A I K A N"
                    android:textColor="@color/ghostwhite"
                    android:background="@drawable/et_bgperbaikanproposal"
                    android:padding="10dp"
                    android:layout_marginBottom="20dp"
                    android:enabled="false"/>
                <Button
                    android:id="@+id/btnTolakProposal_detailProposal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="T O L A K"
                    android:textColor="@color/ghostwhite"
                    android:background="@drawable/et_bgtolakproposal"
                    android:padding="10dp"
                    android:layout_marginBottom="20dp"
                    android:enabled="false"/>
                <Button
                    android:id="@+id/btnUploadBerkasProposal_detailProposal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="U P L O A D   U L A N G"
                    android:textColor="@color/ghostwhite"
                    android:background="@drawable/et_bguploadulangberkas"
                    android:padding="10dp"
                    android:layout_marginBottom="20dp"
                    android:enabled="false"/>
                <ProgressBar
                    android:id="@+id/progressBarDetailProposal"
                    android:layout_width="wrap_content"
                    android:layout_height="100dp"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_marginTop="20dp"
                    android:indeterminate="false"
                    android:max="100"
                    android:progress="0"
                    android:progressDrawable="@drawable/progressbar_custom_lihat_mahasiswa"
                    android:secondaryProgress="0"
                    android:visibility="gone"/>
            </LinearLayout>
        </RelativeLayout>
    </androidx.core.widget.NestedScrollView>

</RelativeLayout>